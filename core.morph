            ]
        },
        {
            "name": "bash",
            "repo": "upstream:bash",
            "ref": "baserock/morph",
            "build-depends": [
                "readline"
            ]
        },
        {
            "name": "error-perl-tarball",
            "repo": "upstream:error-perl-tarball",
            "ref": "baserock/morph",
            "build-depends": [
                "perl"
            ]
        },
        {
            "name": "flex",
            "repo": "upstream:flex",
            "ref": "baserock/build-essential",
            "build-depends": [
                "m4"
            ]
        },
        {
            "name": "openssl",
            "repo": "upstream:openssl",
            "ref": "baserock/morph",
            "build-depends": [
                "perl"
            ]
        },
        {
            "name": "cpython",
            "repo": "upstream:cpython",
            "ref": "baserock/morph-2.7",
