        {
            "name": "libxml2",
            "repo": "upstream:libxml2",
            "ref": "baserock/morph",
            "build-depends": [
                "autoconf",
                "automake",
                "cpython",
                "libtool"
            ]
        },
        {
            "name": "curl",
            "repo": "upstream:curl",
            "ref": "baserock/morph",
            "build-depends": [
                "autoconf",
                "automake",
                "libtool",
                "openssl"
            ]
        },
        {
            "name": "libexpat",
            "repo": "upstream:libexpat",
