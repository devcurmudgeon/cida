  - python-ttystatus
- name: pylru
  repo: upstream:python-packages/pylru
  ref: 221dd259f5c34562c835611d1cf62384b9019da4
  unpetrify-ref: master
- name: morph
  repo: baserock:baserock/morph
  ref: 2579391b05758940652a8ac5207d0137720affd1
  unpetrify-ref: master
  build-depends:
  - cmdtest
  - pyfilesystem
  - pylru
